{
  "name": "snappyshot",
  "version": "0.0.6",
  "description": "Automatically generate snapshot tests for React components and vanilla JavaScript",
  "keywords": [
    "snaphsot",
    "unit tests",
    "test generation"
  ],
  "main": "dist/index.js",
  "scripts": {
    "copy": " cp -r dist/ /usr/local/lib/node_modules/snappyshot/dist",
    "dev": "rimraf dist/ && babel --watch src/ --presets es2015 --out-dir dist/ --ignore __tests__,__mocks__ && npm run copy",
    "test": "node --harmony ./node_modules/.bin/jest",
    "build": "babel-node"
  },
  "bin": {
    "snappyshot": "dist/snappyshot-cli.js"
  },
  "repository": {
    "type": "git",
    "url": "github.com/lyleg/snappyshot"
  },
  "author": "Lyle Garza",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.11.4",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.10.3",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2017-node7": "^0.4.1",
    "babel-preset-stage-0": "^6.16.0",
    "flow-bin": "^0.37.4",
    "jest": "^18.0.0",
    "react": "^15.3.2",
    "react-test-renderer": "^15.3.2"
  },
  "dependencies": {
    "ast-types": "^0.9.3",
    "await-to-js": "^1.0.0",
    "babylon": "^6.14.1",
    "denodeify": "^1.2.1",
    "mkdirp": "^0.5.1",
    "node-dir": "^0.1.16",
    "nomnom": "^1.8.1",
    "prettyjson": "^1.1.3",
    "react-docgen": "^2.10.0",
    "recast": "^0.11.18",
    "recursive-readdir": "^2.1.0",
    "rimraf": "^2.5.4"
  }
}
